
.container {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100vh;
  background-color: #f5f5f5;
}
.button-container {
  text-align: center;
}
.start-button {
  background-color: #007aff;
  color: #ffffff;
  padding: 30rpx 60rpx;
  border-radius: 50rpx;
  font-size: 36rpx;
  border: none;
}

/* 小程序中不支持 :hover，可以使用 :active 代替 */
.start-button:active {
  opacity: 0.8;
}
